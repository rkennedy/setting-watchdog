{
  "name": "setting-watchdog",
  "version-string": "1.0.0",
  "description": "Reset OS settings when they change.",
  "dependencies": [
    "boost-algorithm",
    "boost-any",
    "boost-core",
    "boost-dll",
    "boost-nowide",
    "boost-numeric-conversion",
    "boost-program-options",
    "boost-range",
    {
      "name": "glog",
      "default-features": false
    }
  ],
  "$modeline": " vim: set et sw=2 tw=2 ts=2 : "
}
